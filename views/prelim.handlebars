    <!-- Employee Tracker -->
    <div class="container">
	
      <!-- Jumbotron -->
      <div class="jumbotron" style="background-color: black; color: white">
        <h1 class="text-center">{{round}} Competition</h1>
        <h2 class="text-center">{{division}}</h2>
        <h2 class="text-center">{{role}}</h2>
      </div>

      <div class="row">

        <div class="col-lg-12">

        <form method="POST">
        	<div class="form-group">
          	<div class="panel panel-primary">
            <div class="panel-heading">
              <h3 class="panel-title"><strong>Heat One</strong></h3>
            </div>
            <div class="panel-body">
              <table class="table table-hover" id='prelim-heatOne'>
                <thead>
                  <tr>
                    <th>Bib Number</th>
                    <th>Role</th>
                    <th>Score</th>
                  </tr>
                </thead>
              
                <tbody>
                	{{#each list}}
                	<tr>
    				<td>{{this.bib_number}}</td>
    				<td>{{role}}</td>
    				<td>
    					
						<div class="well">
							<b>YES</b>
            				<input type="text" class="span2" value="3" name= {{this.bib_number}} id="{{this.bib_number}}" data-slider-max="3"
            				data-slider-min="1" >
            	
            				<b>NO</b>
         				</div>

					
					</td>
    				</tr>
  					{{/each}}

                </tbody>
                </table>
            
              
            </div>
          	</div>
          	</div>
          	<button class="btn btn-primary" id="score-prelims-btn" type="submit">Submit Scores</button>
        </form>
        </div>

      </div>

    </div>

<!-- BELOW CODE IS CRITICAL. IT HANDLES HOW FORM DATA IS SENT TO OUR SERVER -->
<script type="text/javascript">

for(var i = 100; i < 1000; i++) {

	if($('#' +i)[0]) {
		$('#' +i).slider();
	}

}





</script>